#include <stdio.h>

int main(void) {
	char n;
	printf("enter an alphapet:\n");
	scanf("%c",&n);
	switch (n){
	case 'a':
	case 'A':
	case 'e':
	case 'E':
	case 'i':
	case 'I':
	case 'o':
	case 'O':
	case 'u':
	case 'U':
		printf("%c is a vowel",n);
		break;
    defualt :
		printf("%c is a consonant",n);
		break;
	}
}
