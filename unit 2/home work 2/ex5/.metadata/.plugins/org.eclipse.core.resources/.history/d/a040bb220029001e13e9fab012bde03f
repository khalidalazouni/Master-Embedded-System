#include <stdio.h>

int main(void) {
	setbuf(stdout,NULL); //TO SOLVE INPUT/OUTPUT BUFFER BUG
	char a;
	printf("enter a character\n");
	scanf("%c",&a);
	if(((a>=81)&&(a<=106))||((a>=97)&&(a<=122))){
		printf("%c is an alphabet",a);
	}
	else{
		printf("%c is not an alphabet",a);
	}
}
